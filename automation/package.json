{
  "name": "share-automation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "sit:au": "bash -c \"./node_modules/.bin/chimp chimp.shippable.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@sitAU\"",
    "test:mobile": "bash -c \"APP_KEY=now-mobile node runner.js --tags=@mobile --pMode=mobile\"",
    "test:desktop": "bash -c \"APP_KEY=now-desktop node runner.js --tags=@desktop\"",
    "test:tags": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=$0\"",
    "test:high": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=@high\"",
    "test:med": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=@med\"",
    "test:low": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=@low\"",
    "test:nzlow": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@nznow --tags=@low\"",
    "test:rerun": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs @rerun.txt\"",
    "secure:rerun": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=chrome @rerun.txt\"",
    "all:test": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=~@cms --tags=~@manual\"",
    "test:secure": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=chrome --format=rerun:@rerun.txt --tags=@now --tags=~@cms --tags=~@manual\"",
    "test:cms": "bash -c \"./node_modules/.bin/chimp chimp.cms.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@cms --tags=~@manual\"",
    "test:cmsprelive": "bash -c \"./node_modules/.bin/chimp chimp.cms.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@cmsprelive --tags=~@manual\"",
    "single": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs\"",
    "once": "npm run single",
    "debug": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --debug\"",
    "watch": "bash -c \"./node_modules/.bin/chimp chimp.js --watch\"",
    "watch:phantom": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --watch\"",
    "features": "bash -c \"node runner.js --pMode=features\"",
    "now:automation": "bash -c \"URL=http://now-site.test.bxm.net.au/ APP_KEY=now-site npm run all:test\"",
    "now:automation:high": "bash -c \"URL=http://now-site.test.bxm.net.au/ APP_KEY=now-site npm run test:high\"",
    "share:aulive": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --tags=@aulive\"",
    "share:nzlive": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --tags=@nzlive\"",
    "general:live": "bash -c \"./node_modules/.bin/chimp chimp.js --browser=phantomjs --tags=@live\"",
    "test:phantom": "bash -c \"URL=http://now-site.test.bxm.net.au/ APP_KEY=now-site npm run features\""
  },
  "dependencies": {
    "@bxm/automation": "^6.6.0"
  },
  "devDependencies": {
    "he": "^1.1.1"
  }
}

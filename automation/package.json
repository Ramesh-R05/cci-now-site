{
  "name": "share-automation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "sit:au": "bxm-automation ./config/chimpy.shippable.js --browser=chrome --format=rerun:@rerun.txt --tags=@sitAU",
    "test:tags": "bxm-automation ./config/chimpy.js --browser=phantomjs --format=rerun:@rerun.txt --tags=$0",
    "test:high": "bxm-automation ./config/chimpy.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=@high",
    "test:med": "bxm-automation ./config/chimpy.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=@med",
    "test:low": "bxm-automation ./config/chimpy.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=@low",
    "test:nzlow": "bxm-automation ./config/chimpy.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@nznow --tags=@low",
    "test:rerun": "bxm-automation ./config/chimpy.js --browser=phantomjs @rerun.txt",
    "test:perf": "mocha --require babel-polyfill --require babel-register --reporter spec --recursive perf/lighthouse-tests.js ",
    "secure:rerun": "bxm-automation ./config/chimpy.js --browser=phantomjs @rerun.txt",
    "all:test": "bxm-automation ./config/chimpy.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=~@cms --tags=~@manual",
    "test:secure": "bxm-automation ./config/chimpy.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@now --tags=~@cms --tags=~@manual",
    "test:cms": "bxm-automation ./config/chimpy.cms.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@cms --tags=~@manual",
    "test:cmsprelive": "bxm-automation ./config/chimpy.cms.js --browser=phantomjs --format=rerun:@rerun.txt --tags=@cmsprelive --tags=~@manual",
    "single": "bxm-automation ./config/chimpy.js --browser=phantomjs",
    "once": "npm run single",
    "debug": "bxm-automation ./config/chimpy.js --browser=phantomjs --debug",
    "watch": "bxm-automation ./config/chimpy.js --watch",
    "watch:phantom": "bxm-automation ./config/chimpy.js --browser=phantomjs --watch",
    "now:automation": "URL=http://now-site.test.bxm.net.au/ APP_KEY=now-site npm run all:test",
    "now:automation:high": "URL=http://now-site.test.bxm.net.au/ APP_KEY=now-site npm run test:high",
    "share:aulive": "bxm-automation ./config/chimpy.js --browser=phantomjs --tags=@aulive",
    "share:nzlive": "bxm-automation ./config/chimpy.js --browser=phantomjs --tags=@nzlive",
    "general:live": "bxm-automation ./config/chimpy.js --browser=phantomjs --tags=@live",
    "test:phantom": "URL=http://now-site.test.bxm.net.au/ APP_KEY=now-site npm run features"
  },
  "dependencies": {
    "@bxm/automation": "^8.0.0",
    "chai": "^2.1.2",
    "dotenv": "^8.0.0"
  },
  "devDependencies": {
    "he": "^1.1.1"
  }
}

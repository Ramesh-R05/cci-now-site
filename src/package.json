{
  "private": true,
  "name": "now-site",
  "version": "1.0.0",
  "description": "Now To Love web site",
  "main": "start.js",
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "prepush": "run-p lint test",
    "start": "node ./start.js",
    "test": "babel-node ./node_modules/babel-istanbul/lib/cli cover --root ./ --include-all-sources --report teamcity --report cobertura --report html ./node_modules/mocha/bin/_mocha -- --require ./test/setup.js --recursive --check-leaks",
    "test:ci": "npm run test -- --reporter=xunit-file",
    "test:unit": "mocha --require babel-polyfill --require babel-register --require test/setup.js --reporter min --recursive --check-leaks",
    "test:tdd": "npm run test:unit -- --watch",
    "clean": "rimraf dist/ coverage/ .build/ && mkdirp dist/scripts dist/styles .build/",
    "build": "npm-run-all -s clean -p build:css build:js",
    "build:css": "node-sass --output-style compressed app/styles/main.scss > .build/_main.css && postcss --use autoprefixer --autoprefixer.browsers \"last 2 versions, ie >= 10, ios >= 7, Android >= 4\" --output dist/styles/main.css .build/_main.css",
    "build:js": "browserify app/client.js --debug --transform babelify --plugin [ minifyify --no-map ] --ignore-missing --external request --outfile ./dist/scripts/main.js",
    "dev": "npm-run-all -s clean -p dev:css dev:js dev:serve",
    "dev:serve": "nodemon --watch app start.js",
    "dev:css": "nodemon --watch app/styles --ext scss --exec \"npm run build:css\"",
    "dev:js": "watchify app/client.js --debug --transform babelify --ignore-missing --external request --verbose --outfile dist/scripts/main.js",
    "stubbed": "cross-env NODE_ENV=stubbed npm run dev",
    "lint:scss": "sass-lint --config .sass-lint.yml --ignore './**/vendor/**' --no-exit --verbose './app*/styles/**/*.scss'",
    "lint:js": "eslint --fix --ext .js app/",
    "lint": "run-p lint:scss lint:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bauerxcelmedia/now-site.git"
  },
  "author": "Bauer Xcel Media",
  "license": "",
  "homepage": "https://github.com/bauerxcelmedia/now-site#readme",
  "dependencies": {
    "@bxm/ad": "^10.3.1",
    "@bxm/article": "^21.8.0",
    "@bxm/behaviour": "^1.2.0",
    "@bxm/config": "^4.1.1",
    "@bxm/datetime": "^3.0.0",
    "@bxm/flux": "^4.2.0",
    "@bxm/gallery": "^14.1.1",
    "@bxm/header": "^11.0.1",
    "@bxm/markdown": "^1.2.0",
    "@bxm/modal": "^2.1.0",
    "@bxm/nav": "^3.1.1",
    "@bxm/newsletter": "^3.0.0",
    "@bxm/node-apm": "^1.0.1",
    "@bxm/react-script-loader": "^2.1.0",
    "@bxm/recommendations": "^4.1.0",
    "@bxm/server": "^20.0.0",
    "@bxm/services-stubs": "^2.3.0",
    "@bxm/site-header": "^2.4.0",
    "@bxm/social": "^1.5.0",
    "@bxm/tags": "^3.0.0",
    "@bxm/teaser": "^5.2.2",
    "@bxm/ui": "^15.0.1",
    "@bxm/video": "^4.0.0",
    "@bxm/winston-logger": "^1.1.0",
    "babel-core": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-register": "^6.6.5",
    "classnames": "^2.1.2",
    "console-shim": "^1.0.3",
    "cross-env": "^1.0.7",
    "exenv": "^1.2.0",
    "express": "^4.13.3",
    "fluxible": "^1.0.3",
    "fluxible-addons-react": "^0.2.2",
    "fluxible-reducer-store": "^0.1.0",
    "fluxible-router": "^0.4.11",
    "lodash": "^3.10.1",

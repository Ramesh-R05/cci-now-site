{
  "private": true,
  "name": "now-site",
  "version": "1.0.0",
  "description": "Now To Love web site",
  "main": "start.js",
  "engines": {
    "node": ">=6.1.0",
    "npm": ">=3.0.0"
  },
  "scripts": {
    "prepush": "run-p lint test",
    "start": "NODE_ENV=production node $NODE_DEBUG_OPTION ./start.js",
    "test": "babel-node ./node_modules/babel-istanbul/lib/cli cover --root ./app --include-all-sources --report cobertura --report html ./node_modules/mocha/bin/_mocha -- --require ./test/setup.js --recursive --check-leaks",
    "test:ci": "npm run test -- --reporter=xunit --reporter-options output=./../shippable/testresults/result.xml",
    "test:unit": "mocha --require babel-polyfill --require babel-register --require test/setup.js --reporter min --recursive --check-leaks",
    "test:tdd": "npm run test:unit -- --watch",
    "test:perf": "mocha --require babel-polyfill --require babel-register --require test/setup.js --reporter spec --recursive perf/*.js ",
    "clean": "rimraf dist/ coverage/ && mkdir dist",
    "build": "npm run clean && NODE_ENV=production webpack --hide-modules --progress",
    "dev": "npm run clean && webpack --hide-modules --progress --watch & npm run serve",
    "dev:debug": "npm run clean && webpack --hide-modules --progress --watch & npm run serve:debug",
    "dev:win": "npm run clean && webpack --hide-modules --progress && npm run serve",
    "serve": "nodemon --watch app --ext js ./start.js",
    "serve:debug": "nodemon $NODE_DEBUG_OPTION --watch app --ext js ./start.js",
    "lint:scss": "sass-lint --config .sass-lint.yml --ignore './**/vendor/**' --no-exit --verbose './app*/styles/**/*.scss'",
    "lint:js": "eslint --fix --ext .js app/",
    "lint": "run-p lint:scss lint:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bauerxcelmedia/now-site.git"
  },
  "author": "Bauer Xcel Media",
  "license": "UNLICENSED",
  "homepage": "https://github.com/bauerxcelmedia/now-site#readme",
